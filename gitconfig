# This is Git's per-user configuration file.
[user]
	name = Tarak Shah
	email = tarak@hrdag.org
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[color "grep"]
    context = dim white
    function = dim white
    filename = dim white
    linenumber = dim yellow
    match = red
    selected = white
    separator = dim blue
[init]
	defaultBranch = main
